{
    "name": "object_detection",
    "version": 1,
    "type": "FFmpeg",
    "template":"-flags unaligned -hwaccel vaapi -hwaccel_output_format vaapi -hwaccel_device /dev/dri/renderD128 -i \"{source[uri]}\" -vf \"detect=model={models[object_detection][1][network]}:device=6:model_proc=\"{models[object_detection][1][proc]}\":interval={parameters[every-nth-frame]}\"",
    "description": "Object Detection",
    "parameters": {
        "type" : "object",
        "properties" : {
            "every-nth-frame": {
                "element":"detection",
                "type": "integer",
                "minimum": 0,
                "maximum": 4294967295,
                "default": 1
            }
        }
    }
}


