{
    "name": "emotion_recognition",
    "version": 1,
    "type": "FFmpeg",
    "template":"-flags unaligned -hwaccel vaapi -hwaccel_output_format vaapi -hwaccel_device /dev/dri/renderD128 -i \"{source[uri]}\" -vf \"detect=model={models[face_detection_retail_fp16][1][network]}:model_proc=\"{models[face_detection_retail_fp16][1][proc]}\":interval=\"{parameters[every-nth-frame]}\":device=6,classify=model=\"{models[emotion_recognition_fp16][1][network]}\":model_proc=\"{models[emotion_recognition_fp16][1][proc]}\":device=6\"",
    "description":"Emotion Recognition",
    "parameters": {
	"every-nth-frame":{
	    "default":1
	}
    }
}


